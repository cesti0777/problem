package samsung.practice1;

import java.util.LinkedList;
import java.util.Queue;
import java.util.Scanner;

public class P13460_new {
	static int r;
	static int c;
	static char map[][] = new char[10][10];
	static int rR, rC, bR, bC;
	
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		
		r = in.nextInt();
		c = in.nextInt();
		in.nextLine();
		
		for(int i = 0; i < r; i++){
			String line = in.nextLine();
			for(int j = 0; j < c; j++){
				map[i][j] = line.charAt(j);
				if(map[i][j] == 'R'){
					rR = i;
					rC = j;
				} else if(map[i][j] == 'B'){
					bR = i;
					bC = j;
				}
			}
		}
		
		//print(map);
		
		Queue<Data> q = new LinkedList<Data>();
		q.add(new Data(rR, rC, bR, bC));
		
		while(!q.isEmpty()){
			Data data = q.remove();
			
			//R 굴리기, 공 겹치는 경우 고려 x
			while(true){
				
			}
			
			//B 굴리기, 공 겹치는 경우 고려 x
			while(true){
				
			}
		}
	}
	
	
	public static void print(char[][] arr){
		System.out.println();
		for(int i = 0; i < r; i++){
			for(int j = 0; j < c; j++){
				System.out.print(arr[i][j]);
			}
			System.out.println();
		}
	}

	public static class Data{
		int rR, rC, bR, bC;
		
		public Data() {}
		
		public Data(int rR, int rC, int bR, int bC) {
			this.rR = rR;
			this.rC = rC;
			this.bR = bR;
			this.bC = bC;
		}
	}
}
