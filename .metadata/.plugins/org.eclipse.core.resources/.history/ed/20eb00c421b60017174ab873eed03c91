package samsung.review;

import java.util.LinkedList;
import java.util.Queue;
import java.util.Scanner;

public class P14503 {
	static int testCnt;
	static int answer;
	static int n;
	static int m;
	static int r;
	static int c;
	static int d;
	static int[][] map;
	static boolean[][] visited;
	
	final static int N = 0;
	final static int E = 1;
	final static int S = 2;
	final static int W = 3;
	
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		
		testCnt = in.nextInt();
		
		for(int testCase = 1; testCase <= testCnt; testCase++){
			
			init();
			
			n = in.nextInt();
			m = in.nextInt();
			r = in.nextInt();
			c = in.nextInt();
			d = in.nextInt();
			
			for(int i = 0; i < n; i++){
				for(int j = 0; j < m; j++){
					map[i][j] = in.nextInt();
				}
			}
				
			execute();
			
			System.out.println("#" + testCase + " " + answer);
			
		}
		
		
		
		
	}
	
	public static void init(){
		answer = n = m = r = c = d = 0;
		map = null;
		map = new int[50][50];
		visited = null;
		visited = new boolean[50][50];
	}
	
	public static void execute(){
		Queue<Integer> q = new LinkedList<Integer>();
		
		
		visited[r][c] = true; // 초기 위치 청소
		q.add(r);
		q.add(c);
		q.add(d);
		q.add(answer);
		
		int curR = 0;
		int curC = 0;
		int curD = 0;
		int no = 0;
		while(!q.isEmpty()){
			curR = q.remove();
			curC = q.remove();
			curD = q.remove();
			answer = q.remove();
			
			
			switch (curD) {
			case N://북쪽 바라보는 중
				if(map[curR][curC - 1] == 0 && visited[curR][curC - 1]){//왼쪽이 빈칸 and 청소x
					curD = W;//왼쪽으로 회전
					curC--;//한 칸 전진
					visited[curR][curC] = true;
					break;
				}else{//왼쪽 벽 or 청소o
					curD = W;//왼쪽으로 회전
					no++;
					break;
				}
				break;
			case E://동쪽 바라보는 중
				if(map[curR - 1][curC] == 0 && visited[curR - 1][curC]){//왼쪽 청소 안했으면(빈칸이면)
					curD = W;//왼쪽으로 회전
					curC--;//한 칸 전진
					visited[curR][curC] = true;
					break;
				}else{//왼쪽 벽 or 청소o
					curD = W;//왼쪽으로 회전
					no++;
					break;
				}
				break;
			case S://남쪽 바라보는 중
				if(map[curR][curC + 1] == 0 && visited[curR][curC + 1]){//왼쪽 청소 안했으면(빈칸이면)
					curD = W;//왼쪽으로 회전
					curC--;//한 칸 전진
					visited[curR][curC] = true;
					break;
				}else{//왼쪽 벽 or 청소o
					curD = W;//왼쪽으로 회전
					no++;
					break;
				}
				break;
			case W://서쪽 바라보는 중
				if(map[curR + 1][curC] == 0 && visited[curR + 1][curC]){//왼쪽 청소 안했으면(빈칸이면)
					curD = W;//왼쪽으로 회전
					curC--;//한 칸 전진
					visited[curR][curC] = true;
					break;
				}else{//왼쪽 벽 or 청소o
					curD = W;//왼쪽으로 회전
					no++;
					break;
				}
				break;
			}
		}
	}
}
