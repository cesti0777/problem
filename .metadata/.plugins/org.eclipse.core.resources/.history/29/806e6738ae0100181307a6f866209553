package study;

import java.util.Arrays;
import java.util.Scanner;

public class SE1860 {

	static int testCaseCnt;//testcase 개수
	static int n, m ,k;//사람 수, 붕어빵 생성 주기, 붕어빵 생성량
	static int[] people;
	static int fish;//현재 붕어 갯수
	static int totalSec;
	static String answer;

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);

		testCaseCnt = in.nextInt();//testcase 개수 입력

		for (int testCase = 1; testCase <= testCaseCnt; testCase++) {
			
			init();//변수 초기화
			
			n = in.nextInt();//사람 수 입력
			m = in.nextInt();//붕어빵 생성 주기 입력
			k = in.nextInt();//붕어빵 생성량 입력
			
			people = new int[n];//사람 도착 시간
			
			for(int i = 0; i < n; i++){//사람 도착 시간 입력
				people[i] = in.nextInt();
			}
			
			Arrays.sort(people);//사람 도착 시간 오름차순 정렬
			
			execute();//실행
			
			System.out.println("#" + testCase + " " + answer);
		}
	}

	public static void init() {
		fish = 0;
		totalSec = 0;
		people = null;
	}

	public static void execute() {
		int pIdx = 0;//몇번째 사람
		int fishSec = m;//처음 붕어 나오는시간
		for(int i = 0; i <= totalSec; i++){
			if(i == fishSec){
				fish += k;
				fishSec += m;
			}
			if(people[pIdx] == i){
				if(fish - 1 < 0){
					answer = "Impossible";
					return;
				}else{
					fish--;
					pIdx++;
				}
			}
			if(pIdx == n){
				answer = "Possible";
			}
		}
	}
}
