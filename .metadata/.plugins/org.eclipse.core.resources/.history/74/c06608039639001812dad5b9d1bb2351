package kj_ss;

import java.util.Scanner;

public class s2817 {
	static int n;
	static int k;
	static int t;
	static int[] arr;
	static int count;

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		t = sc.nextInt();
		for (int i = 0; i < t; i++) {
			n = sc.nextInt();
			k = sc.nextInt();
			arr = new int[n];
			for (int j = 0; j < n; j++) {
				arr[j] = sc.nextInt();
			}
			for (int k = 0; k < n; k++) {
				run(k, 0);
			}
		}
		System.out.println(count);
	}

	public static void run(int start, int sum) {
		for (int i = 1; i < n - start + 1; i++) {
			run(start + i, sum + arr[start]);
		}
		if (start > n) {
			if (sum == k) {
				count++;
				return;
			} else
				return;
		}
	}

}
