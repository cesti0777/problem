package samsung.review;

import java.util.Scanner;

public class P14502 {
	static int answer;
	static int testCnt;
	static int n;
	static int m;
	static int[][] map;
	static boolean[][] visited;
	
	
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		
		testCnt = in.nextInt();
		
		for(int testCase = 1; testCase <= testCnt; testCase++){
			init();
			
			n = in.nextInt();
			m = in.nextInt();
			
			for(int i = 0; i < n; i++){
				for(int j = 0; j < m; j++){
					map[i][j] = in.nextInt();
				}
			}
			
			printMap();
			
			execute();
			
			System.out.println("#" + testCase + " " + answer);
		}
		
	}
	
	public static void init(){
		answer = n = m = 0;
		map = null;
		visited = null;
		map = new int[8][8];
		visited = new boolean[8][8];
	}
	
	public static void execute(){
		//¸ÊÀ» 1Â÷¿øÀ¸·Î ÀúÀå
		int[] platMap = new int[n * m];
		for(int i = 0; i < n; i++){
			for(int j = 0; j < m; j++){
				platMap[m * i + j] = map[i][j];
			}
		}
		
//		int wallR1 = 0;
//		int wallC1 = 0;
//		int wallR2 = 0;
//		int wallC2 = 0;
//		int wallR3 = 0;
//		int wallC3 = 0;
//		
//		for(int i = 0; i < n * m - 2; i++){//º®1 »Ì±â
//			for(int j = i + 1; j < n * m - 1; j++){//º®2 »Ì±â
//				for(int k = j + 1; k < n * m; k++){//º®3 »Ì±â
//					if(platMap[i] == 0 && platMap[j] == 0 && platMap[k] == 0){
//						wallX1 = i/n
//					}
//				}
//			}
		}
		
		
		
		
		
//		
//		for(int i = 0; i < n; i++){
//			for(int j = 0; j < m; j++){
//				System.out.print(platMap[n * i + j] + " ");
//			}
//			System.out.println();
//		}
	}
	
	public static void printMap(){
		for(int i = 0; i < n; i++){
			for(int j = 0; j < m; j++){
				System.out.print(map[i][j] + " ");
			}
			System.out.println();
		}
	}
}
