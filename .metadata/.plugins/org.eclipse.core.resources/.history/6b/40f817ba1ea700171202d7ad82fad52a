package midas.test;

import java.util.Scanner;

public class Program {
	static int n = 0;
	static int[][] arr = new int[21][21]; 
	
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);

		n = in.nextInt();
		in.nextLine();
		
		for(int i = 1; i <= n; i++){
			for(int j = 1; j <= n; j++){
				arr[i][j] = in.nextInt();
			}
		}
		
		for(int i = 1; i <= n; i++){
			for(int j = 1; j <= n; j++){
				System.out.print(arr[i][j]);
			}
			System.out.println();
		}
		
	}

}
