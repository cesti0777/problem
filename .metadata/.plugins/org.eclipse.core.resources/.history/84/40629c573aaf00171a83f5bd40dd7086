package samsung.practice1;

import java.util.Scanner;

public class P14501 {
	
	static int n;
	static int[][] day = new int[15][2];

	public static void main(String[] args) {
		
		Scanner in = new Scanner(System.in);
		
		//day 갯수 입력
		n = in.nextInt();
		
		//day 정보 입력
		for(int i = 0; i < n; i++){
			day[i][0] = in.nextInt();
			day[i][1] = in.nextInt();
		}
		
		for(int i = 0; i < n; i++){
			System.out.println(day[i][0] +" "+ day[i][1]);
		}
	}

}
