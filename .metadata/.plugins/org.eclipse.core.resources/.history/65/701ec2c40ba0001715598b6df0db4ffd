import java.util.Scanner;
class Main {

  public static void main(String[] args) {
Scanner sc = new Scanner(System.in);
         String n = sc.nextLine();
         String[] num = n.split("\\s");
         String[] back = new String[3];
         int j=0;
         boolean trash = false;
         for(int i=0;i<num.length;i++){
            if(j!=3){
               back[j] = num[i];
               j++;
            }else{
               boolean same = false;
               int samepo=0;
               for(int k=0;k<back.length;k++){
                  if(back[k].equals(num[i])){
                     same = true;
                     samepo = k;
                  }
               }
               if(same)
               {
                  String temp = "";
                  if(samepo==0){
                     temp = back[0];
                     back[0] = back[1];
                     back[1] = back[2];
                     back[2] = temp;
                  }
                  else if(samepo==1){
                     temp = back[1];
                     back[1] = back[2];
                     back[2] = temp;
                  }   
                  else{   
                  }   
               }else{
                  trash = true;
                  System.out.println(back[0]);
                  back[0] = back[1];
                  back[1] = back[2];
                  back[2] = num[i];
               }
                  
            }
         }
         if(!trash)
            System.out.println("0");
     }
}