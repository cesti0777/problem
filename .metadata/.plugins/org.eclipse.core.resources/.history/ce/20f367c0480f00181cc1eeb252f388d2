package study;

import java.util.Scanner;

public class SE1493 {

	static int testCaseCnt;//testcase 개수
	static int answer;
	static int n;
	static int p, q;
	static int[][] map = new int[151][151];

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		
		testCaseCnt = in.nextInt();//testcase 개수 입력

		for (int testCase = 1; testCase <= testCaseCnt; testCase++) {
			
			p = in.nextInt();
			q = in.nextInt();
			
			execute();//실행
			
			System.out.println("#" + testCase + " " + answer);
		}
	}
	
	public static void execute() {
		
		//수열 생성
		int rS = 0;
		int cS = 0;
		int rD = 1;
		int cD = 1;
		int[] pStep1 = new int[2];
		int[] qStep1 = new int[2];
		for(int i = 1; i <= 150; i++){
			rS += rD;
			if(rS >= 10000){
				System.out.println(i + " " + rS);
				return;
			}
			cS = rS;
			cD = rD;
			for(int j = 1; j <= 150; j++){
				if(cS == p){
					pStep1[0] = i;
					pStep1[1] = j;
				}else if(cS == q){
					qStep1[0] = i;
					qStep1[1] = j;
				}
				map[i][j] = cS;
				cS += cD;
				cD++;
			}
			cD = 0;
			rD++;
		}
		
		print(5);
		
		//계산	
		answer = map[pStep1[0] + qStep1[0]][pStep1[1] + qStep1[1]];
	}
	
	public static void print(int n) {
		for(int i = 0; i <= n; i++){
			for(int j = 0; j <= n; j++){
				System.out.print(map[i][j] + " ");
			}
			System.out.println();
		}
	}
}