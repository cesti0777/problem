package study;

import java.util.Scanner;

public class SE1493 {

	static int n;
	static int[][] map;

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		
		n = in.nextInt();
		
		map = new int[n][n];
		
		int rS = 0;
		int cS = 0;
		int rD = 1;
		int cD = 1;
		for(int i = 0; i < n; i++){
			rS += rD;
			cS = rS;
			cD = rD;
			for(int j = 0; j < n; j++){
				map[i][j] = cS;
				cS += cD;
				cD++;
			}
			cD = 0;
			rD++;
		}
		
		for(int i = 0; i < n; i++){
			for(int j = 0; j < n; j++){
				System.out.print(map[i][j] + " ");
			}
			System.out.println();
		}
	}
}