package study;

import java.util.Scanner;

public class SEbase {

	static int n;
	static int[][] map;

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		
		n = in.nextInt();
		
		int rS = 1;
		int rD = 0;
		int cD = 0;
		for(int i = 0; i < n; i++){
			rS += rD;
			for(int j = 0; j < n; j++){
				map[i][j] = rS + cD;
				cD++;
			}
			rD++;
		}
		
		for(int i = 0; i < n; i++){
			for(int j = 0; j < n; j++){
				System.out.print(map[i][j] + " ");
			}
			System.out.println();
		}
	}
}