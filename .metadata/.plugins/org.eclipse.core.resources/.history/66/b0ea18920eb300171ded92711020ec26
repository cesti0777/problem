package samsung.practice1;

import java.util.Scanner;

public class P9328 {
	static int tCnt;
	static int[][] r = new int[100][100];
	static int[][] c = new int[100][100];
	static int[][][] map = new int[100][104][104]; 
	
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		
		tCnt = in.nextInt();
		for(int m = 0; m < tCnt; m++){
			r = in.nextInt();
			c = in.nextInt();
			for(int i = 0; i < r + 4; i++){
				for(int j = 0; j < c + 4; j++){
					if(i == 0 || i == r + 3 || j == 0 || j == c + 3){
						map[][][]
					}
				}
			}
		}
		
	}

}
