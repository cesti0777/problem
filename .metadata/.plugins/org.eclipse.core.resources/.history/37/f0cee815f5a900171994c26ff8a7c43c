package samsung.practice1;

import java.util.Scanner;

public class P14503 {
	static int map[][] = new int[50][50];
	static boolean visited[][] = new boolean[50][50];
	static int n, m;// 가로세로
	static int r, c;// 위치
	static int d;// 방향

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);

		n = in.nextInt();
		m = in.nextInt();
		r = in.nextInt();
		c = in.nextInt();
		d = in.nextInt();

		for (int i = 0; i < n; i++) {
			for (int j = 0; j < m; j++) {
				map[i][j] = in.nextInt();
			}
		}

		for (int i = 0; i < n; i++) {
			for (int j = 0; j < m; j++) {
				System.out.print(map[i][j] + " ");
			}
			System.out.println();
		}
		int no = 0;
		while (true) {
			visited[r][c] = true;
			
			switch (d) {
			case 0: //북(상)
				if(map[r][c-1] == 0){
					d = 3;
					c = c - 1;
					break;
				}else{
					d = 3;
					no++;
					break;
				}
			case 1: //동(우)

			case 2: //남(하)

			case 3: //서(좌)
			}

		}

	}

}
