package problem.for_;

import java.util.Scanner;

public class p11720 {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);

		try{
			String n1 = in.nextLine();
			String n2 = in.nextLine();
			long result = 0L;
			
			for(long i = n1; i > 0 ; i--){
				result += n2 % 10L;
				n2 = n2 / 10L;
			}
			System.out.println(result);
		}finally {
			in.close();
			in = null;
		}
	}
}
