package samsung.SWtest;

import java.util.Scanner;

public class P2477 {
	static int n;//접수 창구 개수
	static int m;//정비 창구 개수
	static int k;//방문 고객 수
	static int a;//타겟의 접수 창구 번호
	static int b;//타겟의 정비 창구 번호
	static int[] aT = new int[9];
	static int[] bT = new int[9];
	static int[] kT = new int[1000];
	static int i;
	
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		
		n = in.nextInt();
		m = in.nextInt();
		k = in.nextInt();
		a = in.nextInt();
		b = in.nextInt();
		
		for(int i = 0; i < n; i++){
			aT[i] = in.nextInt();
		}
		for(int i = 0; i < m; i++){
			bT[i] = in.nextInt();
		}
		for(int i = 0; i < k; i++){
			kT[i] = in.nextInt();
		}
		
		
	}
}


//7 10
//2 2 6 1 2
//3 2
//4 2
// 두 번째 테스트 케이스, N=2, M=2, K=6, A=1, B=2,본문예제
// a1=3, a2=2
// b1=4, b2=2
// t1=0, t2=0, t3=1, t4=2, t5=3, t6=4
