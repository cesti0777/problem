package problem.cal;

import java.util.Scanner;

public class p2839 {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		
		int n = in.nextInt();
		int cnt = 0;
		
		if(n%5 != 0){
			
		}else{
			cnt = n/5;
		}
	
		System.out.println(cnt);
		
		in.close();
		in = null;
	}
}
