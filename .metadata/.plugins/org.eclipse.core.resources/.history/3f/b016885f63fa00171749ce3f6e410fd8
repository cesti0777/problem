package study;

import java.util.Scanner;

public class p1 {

	static int testCnt;
	static int answer;
	
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		testCnt = in.nextInt();

		for (int testCase = 1; testCase <= testCnt; testCase++) {

			init();

			n = in.nextInt();
			m = in.nextInt();
			k = in.nextInt();
			a = in.nextInt();
			b = in.nextInt();

			for (int i = 1; i <= n; i++) {
				aT[i] = in.nextInt();
			}
			for (int i = 1; i <= m; i++) {
				bT[i] = in.nextInt();
			}
			for (int i = 1; i <= k; i++) {
				kT[i] = in.nextInt();
			}

			execute();
			
			for(int i = 1; i <= k; i++){
				if(visited[i][0] == a && visited[i][1] == b){
					answer += i;
				}
			}
			
			if(answer == 0){
				answer = -1;
			}

			System.out.println("#" + testCase + " " + answer);
		}

	}
	
	public static void init() {
		
	}

	public static void execute(){
		
	}

}
