package problem.for_;

import java.util.Scanner;

public class p2438 {
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		try{
			int n = in.nextInt();
			for(int i = 1; i <= n; n++){
				for(int j = 1; j <= i ; j++){
					System.out.print("*");
				}
			}
		}finally {
			in.close();
			in = null;
		}
	}
}
