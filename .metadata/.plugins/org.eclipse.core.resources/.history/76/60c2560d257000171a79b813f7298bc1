package problem.for_;

import java.util.Scanner;

public class p11720 {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);

		try{
			int n1 = in.nextInt();
			String n2 = in.nextLine();
			long result = 0;
			
			for(int i = n1; i > 0 ; i--){
				result += Integer.parseInt(n2, i);
			}
			System.out.println(result);
		}finally {
			in.close();
			in = null;
		}
	}
}
