package problem.cal;

import java.util.Scanner;

public class p2839 {

	public static void main(String[] args) {
		//상근이가 설탕을 정확하게 N킬로그램 배달해야 할 때, 봉지 몇 개를 가져가면 되는지 그 수를 구하는 프로그램을 작성하시오.\
		Scanner in = new Scanner(System.in);
		
		int n = in.nextInt();
		int cnt = 0;
		
		cnt += n/5;
		n = n%5;
		cnt += n/3;
		if(n%3 != 0){
			cnt++;
		}
		System.out.println(cnt);
	}
}
