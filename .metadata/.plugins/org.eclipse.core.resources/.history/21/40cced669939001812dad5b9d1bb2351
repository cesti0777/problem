package kj_ss;

import java.util.Scanner;

public class s2817 {
	static int n;
	static int k;
	static int t;
	static int[] arr;
	static int count;

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		t = sc.nextInt();
		for (int i = 0; i < t; i++) {
			n = sc.nextInt();
			k = sc.nextInt();
			arr = new int[n];
			for (int j = 0; j < n; j++) {
				arr[j] = sc.nextInt();
			}
			for (int k = 0; k < n; k++) {
				run(k, arr[k]);
				run(k, 0);
			}
		}
		System.out.println(count);
	}

	public static void run(int start, int sum) {
		if (sum == k) {
			count++;
			return;
		}
		if (start >= n)
			return;
		else {
			for (int i = 1; i < n - start; i++) {
				run(start + i, sum + arr[start + i]);
//				run(start + i, sum);
			}
		}
	}

}
