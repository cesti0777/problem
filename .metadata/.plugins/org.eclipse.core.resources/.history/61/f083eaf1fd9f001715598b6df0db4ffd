package nhn.pretest1;

import java.util.Scanner;

public class P1 {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int n = in.nextInt(); // nxn
		String[] num = null;
		int[][] arr = new int[n][n]; 
		for(int i = 0; i < n; i++){
			num = in.nextLine().split(" ");
			for(int j = 0; j < num.length; j++){
				arr[i][j] = Integer.parseInt(num[j]);
			}
		}
		
		for(int i = 0; i < n; i++){
			for(int j = 0; j < n; j++){
				System.out.print(arr[i][j]);
				if(j != n-1){
					System.out.print(" ");
				}else{
					System.out.println();
				}
			}
			System.out.println();
		}
		
		
	}

}
