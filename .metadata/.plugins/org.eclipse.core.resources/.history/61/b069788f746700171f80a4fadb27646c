package problem.cal;

import java.util.Scanner;

public class p2839 {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		
		int n = in.nextInt();
		int cnt = 0;
		
		cnt += n/5;
		n = n%5;
		cnt += n/3;
		if(n%3 != 0){
			cnt = 0;
			cnt += n/3;
			cnt = -1;
		}
		System.out.println(cnt);
		
		in.close();
		in = null;
	}
}
