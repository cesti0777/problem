package study;

import java.util.Scanner;

public class SE1954 {

	static int testCaseCnt;// testcase 개수
	static int n;
	static int[][] arr;
	static String answer;

	public static void main(String[] args) {

		Scanner in = new Scanner(System.in);

		testCaseCnt = in.nextInt();// testcase 개수 입력

		for (int testCase = 1; testCase <= testCaseCnt; testCase++) {

			n = in.nextInt();

			arr = new int[n][n];

			execute();// 실행

			System.out.println("#" + testCase + " " + answer);
		}
	}

	public static void execute() {
		char d = 'R';
		int c = 0;
		int r = 0;
		int curNum = 1;
		arr[r][c] = curNum;
		
		while(true){
			curNum++;
			
			if(d == 'R' && c + 1 < n){//오른쪽이고 배열 범위면
				if(arr[r][c + 1] != 0){//오른쪽칸이 빈칸이면
					c++;//오른쪽으로 이동
					arr[r][c] = curNum;//숫자 기입
				}else{//오른쪽칸이 빈칸이 아니면
					if(arr[r + 1][c] != 0){//아래가 빈칸이 아니면 종료
						return;
					}
					d = 'D';//아래로 방향 전환
				}
			}else if(d == 'D' && r + 1 < n){//아래이고 배열 범위이면
				if(arr[r][c + 1] != 0){//아래가 빈칸이면
					r++;//아래로 이동
					arr[r][c] = curNum;//숫자 기입
				}else{//아래가 빈칸이 아니면
					d = 'D';//아래로 방향 전환
				}
			}
		}
	}
}
