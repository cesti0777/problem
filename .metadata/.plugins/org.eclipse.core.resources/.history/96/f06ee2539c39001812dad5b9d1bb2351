package kj_ss;

import java.util.Scanner;

public class s2817 {
	static int n;
	static int k;
	static int t;
	static int[] arr;
	static int count;
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		t = sc.nextInt();
		for (int i = 0; i < t; i++) {
			n = sc.nextInt();
			k = sc.nextInt();
			arr = new int[n];
			int[] temp = new int[n];
			for (int j = 0; j < n; j++) {
				arr[j] = sc.nextInt();
			}
			for (int k = 0; k < n; k++) {
				run(k, 0, 0, temp);
				temp[0] = k;
				run(k, arr[k], 1, temp);
			}
		}
		System.out.println(count);
	}

	public static void run(int start, int sum, int cnt, int[] str) {
		if (sum == k) {
			count++;
			for(int x=0; x<n; x++) {
				System.out.print(str[x]);
			}
			return;
		}
		if (cnt == n)
			return;
		else {
			for (int i = 1; i < n - start; i++) {
				run(start + i, sum, cnt, str);
				str[cnt] = k;
				run(start + i, sum + arr[start + i], cnt + 1, str);
			}
		}
	}

}
